{
    "root": true,
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "env": {
        "es2021": true,
        "node": true
    },
    "rules": {
        "no-console": "warn",
        "no-undef": "warn",
        "no-unused-vars": "error",
        "no-unreachable": "warn",
        "no-prototype-builtins": "warn",
        "prettier/prettier": "warn"
    }
}
